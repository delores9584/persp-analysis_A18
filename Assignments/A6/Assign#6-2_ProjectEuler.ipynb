    "def sumdigit(n):\n",
    "    # Define a function that returns the sum of digits in a number\n",
    "    tot=0\n",
    "    while(n>0):\n",
    "        dig=n%10\n",
    "        tot=tot+dig\n",
    "        n=n//10\n",
    "    return tot"
   ]
